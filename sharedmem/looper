mkdir newstats
rm newstats/*
for size in 4 16 64 256 1024 4096 16384 65536 262144 524288
#for size in 256 4096 16384 65536
do
    echo $size
    for type in gettime rdtsc
    do
	echo $type
	echo $i;
	./shmIPC_$type $size >> "newstats/stats_${size}_${type}.txt"
	#./shmIPC_$type $size
	# sleep 1
    done
done
